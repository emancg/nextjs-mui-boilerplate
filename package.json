{
  "name": "nextjs-mui-boilerplate",
  "version": "1.0.0",
  "description": "A comprehensive Next.js + MUI boilerplate with 60+ components, dark mode, and config-driven architecture",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/nextjs-mui-boilerplate.git"
  },
  "keywords": [
    "nextjs",
    "mui",
    "material-ui",
    "boilerplate",
    "template",
    "dark-mode",
    "seo",
    "react",
    "typescript-ready"
  ],
  "scripts": {
    "dev": "next dev",
    "dev:nodemon": "nodemon",
    "dev:3002": "next dev -p 3002",
    "build": "next build",
    "start": "next start",
    "clean": "rimraf .next",
    "clean:full": "rimraf .next node_modules && npm install",
    "kill:node": "taskkill /F /IM node.exe || echo No Node.js processes found",
    "restart": "npm run kill:node && timeout /t 2 && npm run clean && npm run dev",
    "start:clean": "npm run clean && npm run build &&npm run start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "analyze": "ANALYZE=true next build",
    "generate:placeholders": "node scripts/generate-placeholders.js"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^5.2.2",
    "@mui/icons-material": "^5.15.14",
    "@mui/material-nextjs": "^5.15.11",
    "framer-motion": "^12.23.24",
    "next": "14.1.4",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.66.0",
    "react-social-media-embed": "^2.5.9",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "eslint": "^8",
    "eslint-config-next": "14.1.4",
    "nodemon": "^3.1.10",
    "sharp": "^0.34.4"
  }
}
